;;;; personal-information.el
;; for anything sensitive

(setq
 user-full-name "Your Name"
 user-mail-address "Your@email.com"
 )

(defvar irc-password "your_password")

(setq
 my/org-directory "~/Org/"
 my/org-gtd-file (expand-file-name "gtd.org" my-org-directory)
 my/org-todo-file (expand-file-name "todo.org" my-org-directory)
 my/org-journal-file (expand-file-name "journal.org" my-org-directory)
 my/org-remember-file (expand-file-name "remember.org" my-org-directory)
 )

(setq
 my/diary-file "~/.emacs.d/.diary"
 my/bookmark-file "~/.emacs.d/.bookmarks"
 )

(provide 'personal-information)
